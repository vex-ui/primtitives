{
  "name": "@vex-ui/core",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vex-ui.umd.cjs",
  "module": "./dist/vex-ui.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vex-ui.js",
      "require": "./dist/vex-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "types": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "publish": "pnpm publish --no-git-checks --access public",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "up-dep": "taze major -I"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@floating-ui/vue": "^1.0.2",
    "@heroicons/vue": "^2.0.18",
    "@tanstack/table-core": "^8.9.7",
    "@tanstack/vue-table": "^8.9.7",
    "@vex-ui/composables": "^0.1.1",
    "@vueuse/core": "^10.4.1",
    "chartist": "^1.3.0",
    "focus-trap": "^7.5.2",
    "motion": "^10.16.2",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/browser": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.1.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.67.0",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "v-lazy-show": "^0.2.3",
    "vite": "^4.4.9",
    "vite-plugin-vue-devtools": "1.0.0-rc.4",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  }
}
